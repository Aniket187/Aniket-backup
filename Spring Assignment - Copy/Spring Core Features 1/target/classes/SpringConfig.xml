<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	
	
	

<bean id="user" class="bean.User">
<property name="name" value="Karan Mishra"></property>
<property name="city" value="Pune"></property>
<property name="country" value="India"></property>
</bean>


<bean id="user1" class="bean.User">
<constructor-arg index="0" value="Ram"></constructor-arg>
<constructor-arg index="1" value="Akola"></constructor-arg>
<constructor-arg index="2" value="India"></constructor-arg>
</bean>



</beans>
